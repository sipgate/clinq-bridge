{
	"extends": ["tslint:recommended", "tslint:latest", "tslint-config-prettier"],
	"rules": {
		"object-literal-sort-keys": false,
		"typedef": [true, "call-signature", "parameter", "variable-declaration", "member-variable-declaration"],
		"semicolon": true,
		"no-implicit-dependencies": false,
		"interface-name": false,
		"no-any": true,
		"no-console": false,
		"quotemark": [true, "double"]
	}
}
