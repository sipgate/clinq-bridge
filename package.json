{
  "name": "@clinq/bridge",
  "version": "11.2.2",
  "description": "CLINQ Bridge Framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "github:sipgate/clinq-bridge",
  "contributors": [
    {
      "name": "Benjamin Kluck",
      "email": "ben@fug.lu"
    },
    {
      "name": "Felix Gladisch",
      "email": "felix@felixgladisch.de"
    }
  ],
  "bugs": {
    "url": "https://github.com/sipgate/clinq-bridge/issues"
  },
  "keywords": [
    "clinq",
    "crm",
    "express",
    "framework"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "test": "jest",
    "compile": "tsc",
    "build": "npm run lint && npm test && rimraf dist && npm run compile",
    "lint": "tslint 'src/**/*.ts'",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "format": "prettier --write '**/*.ts'",
    "link": "npm link"
  },
  "lint-staged": {
    "*.{ts}": [
      "prettier --write",
      "tslint",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/get-port": "^4.2.0",
    "@types/google-libphonenumber": "^7.4.17",
    "@types/jest": "^25.2.1",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^13.13.4",
    "@types/redis": "^2.8.18",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.2",
    "node-mocks-http": "^1.8.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "ajv": "^6.12.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "google-libphonenumber": "^3.2.9",
    "lru-cache": "^5.1.1",
    "redis": "^3.0.2"
  }
}
